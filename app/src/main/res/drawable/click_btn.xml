<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:color="@color/white" android:dashGap="2dp" android:dashWidth="2dp" android:width="2dp"/>
            <solid android:color="#B1ABAB"/>
            <corners android:radius="37dp"/>
        </shape>
    </item>

    <item android:state_enabled="true">
        <shape android:shape="oval">
            <stroke android:color="@color/white" android:dashGap="1dp" android:dashWidth="1dp" android:width="2dp"/>
            <solid android:color="#FFFFFF"/>
            <corners android:radius="37dp"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <stroke android:color="@color/white" android:dashGap="1dp" android:dashWidth="1dp" android:width="2dp"/>
            <solid android:color="#f4f"/>
            <corners android:radius="37dp"/>
        </shape>
    </item>


</selector>